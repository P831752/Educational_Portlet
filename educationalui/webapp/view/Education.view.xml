												<core:Item key="{coursetypeModel>key}" text="{coursetypeModel>text}" />
											</m:ComboBox>
											<m:Text text=" " width="20px"/>
											<m:Label text="{i18n>gradLabel9}" design="Bold"/>
											<m:Text text="{displayModel>cust_Grade}" visible="{= !${displayModel>editMode} }"/>
											<m:Input value="{displayModel>cust_Grade}" visible="{displayModel>editMode}"/>
										</m:VBox>
										<m:VBox class="sapUiLargeMarginEnd" width="20%">
											<m:Label text="{i18n>gradLabel10}" design="Bold"/>
											<m:Text text="{displayModel>cust_Branch_2}" maxLines="1" visible="{= !${displayModel>editMode} }"/>
											<m:ComboBox value="{displayModel>cust_Branch_2}" visible="{displayModel>editMode}"
												items="{
													path: 'branchModelG>/',
													sorter: { path: 'DefaultValue' }
												}">
												<core:Item key="{branchModelG>ExternalCode}" text="{branchModelG>DefaultValue}" />
											</m:ComboBox>
											<m:Text text=" " width="20px"/>
											<m:Label text="{i18n>gradLabel11}" design="Bold"/>
											<m:Text text="{displayModel>DurationInMonths}" visible="{= !${displayModel>editMode} }"/>
											<m:ComboBox value="{displayModel>DurationInMonths}" visible="{displayModel>editMode}"
												items="{
													path: 'durationModel>/'
												}">
												<core:Item key="{durationModel>key}" text="{durationModel>text}" />
											</m:ComboBox>
										</m:VBox>
										<m:VBox class="sapUiLargeMarginEnd" width="20%">
											<m:Label text="{i18n>gradLabel12}" design="Bold"/>
											<m:Text text="{displayModel>cust_Year_of_Passing}" visible="{= !${displayModel>editMode} }"/>
											<m:ComboBox value="{displayModel>cust_Year_of_Passing}" visible="{displayModel>editMode}"
														items="{
															path: 'yearsModel>/',
															sorter: { path: 'text' }
														}">
												<core:Item key="{yearsModel>key}" text="{yearsModel>text}" />
											</m:ComboBox>
											<m:Text text=" " width="20px"/>
											<m:Label text="{i18n>gradLabel13}" design="Bold"/>
											<m:Text text="{displayModel>cust_Percentage}" visible="{= !${displayModel>editMode} }"/>
											<m:Input value="{displayModel>cust_Percentage}" visible="{displayModel>editMode}"/>
										</m:VBox>
									</m:items>
								</m:HBox>
							</m:VBox>
							<m:Text text=" " width="20px"/>
							<m:Panel headerText="{i18n>attachmentLbl}" expandable="true" expanded="true" visible="{displayModel>editMode}">
								<upload:UploadSet 
								uploadUrl="/upload/"
								maxFileSize="0.2"   
								fileTypes="pdf"
								mediaTypes="application/pdf"
								uploadEnabled="true"
								showIcons="true"
								instantUpload="true"
								mediaTypeMismatch="onMediaTypeMismatch"
								fileSizeExceeded="onFileSizeExceeded"
								uploadCompleted="onUploadCompleted"></upload:UploadSet>
							</m:Panel>
						</m:VBox>
					</blocks>
				</ObjectPageSubSection>
			</ObjectPageSection>
		</sections>
		<footer>
			<m:OverflowToolbar>
				<l:VerticalLayout class="sapUiContentPadding" width="100%">
					<l:content>
						<m:CheckBox class="sapMTextBold" text="" valueState="Information" />
					</l:content>
				</l:VerticalLayout>
			</m:OverflowToolbar>
		</footer>
	</ObjectPageLayout>
</mvc:View>